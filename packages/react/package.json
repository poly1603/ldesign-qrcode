{
  "name": "@ldesign/qrcode-react",
  "version": "3.0.0",
  "description": "React components and hooks for QR code generator",
  "keywords": [
    "qrcode",
    "qr-code",
    "react",
    "react-component",
    "generator",
    "hooks"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./components": {
      "types": "./es/components/index.d.ts",
      "import": "./es/components/index.js",
      "require": "./lib/components/index.cjs"
    },
    "./hooks": {
      "types": "./es/hooks/index.d.ts",
      "import": "./es/hooks/index.js",
      "require": "./lib/hooks/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "dev": "ldesign-builder watch",
    "build": "ldesign-builder build",
    "test": "vitest",
    "demo": "cd demo && pnpm dev",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@ldesign/qrcode-core": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}

